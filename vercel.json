{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/projetos", "destination": "/Projetos.html" },
    { "source": "/curriculo", "destination": "/Curriculo.html" },
    { "source": "/contato",  "destination": "/Contato.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
